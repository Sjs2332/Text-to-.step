{
  "name": "text-to-cad",
  "version": "1.0.0",
  "description": "Generate parametric 3D CAD models from natural language using LLM-powered code generation",
  "private": false,
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "dev": "next dev",
    "dev:api": "cd api && python3 -m uvicorn main:app --reload --port 8080",
    "dev:all": "concurrently -n \"frontend,api\" -c \"blue,green\" \"npm run dev\" \"npm run dev:api\"",
    "build": "next build",
    "start": "next start",
    "start:dev": "npm run setup && npm run dev:all",
    "lint": "eslint",
    "format": "prettier --write .",
    "docker:api": "cd api && docker build -t text-to-cad-api . && docker build -t geometry-runner -f Dockerfile.runner .",
    "setup": "npm install && cd api && python3 -m pip install -r requirements.txt && cd .. && npm run docker:api"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-visually-hidden": "^1.2.4",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "@types/three": "^0.182.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.561.0",
    "next": "^16.1.6",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "tailwind-merge": "^3.4.0",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
